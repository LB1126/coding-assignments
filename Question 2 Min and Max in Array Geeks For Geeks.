class Solution {
    public ArrayList<Integer> getMinMax(int[] arr) {
        ArrayList<Integer> minMax = new ArrayList<>();
        int max = 0;
        int min = Integer.MAX_VALUE;
        for(int i = 0; i < arr.length; i++){
            if(arr[i] > max){
                max = arr[i];
            }
            if(arr[i] < min){
                min = arr[i];
            }
        }
        minMax.add(min);
        minMax.add(max);
        return minMax;
    }
}
