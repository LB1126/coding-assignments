class Solution {
    public ArrayList<Integer> findDuplicates(int[] arr) {
        ArrayList<Integer> seen = new ArrayList<>();
        ArrayList<Integer> duplicates = new ArrayList<>();

        for(int i = 0; i < arr.length; i++){ 
            if(seen.contains(arr[i])){
                duplicates.add(arr[i]);
            } else {
                seen.add(arr[i]);
            }
        }
        return duplicates;
    }
}
