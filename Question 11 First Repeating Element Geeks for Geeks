class Solution {
    public static int firstRepeated(int[] arr) {
        int target = 0;
        int index = -1;
        for(int i = 0; i < arr.length - 1; i++){
            target = arr[i];
            for(int j = i + 1; j < arr.length; j++){
                if(arr[j] == arr[i]){
                    index = i + 1;
                    break;
                }
            }
            if(index != - 1) break;
        }
        return index;
    }
}
